  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.chunk.js",
    "start": "yarn watch-recompile-plugins & yarn watch-plugins-css & yarn build-shared && yarn watch-shared & react-scripts -r @cypress/instrument-cra start",
    "build": "yarn build-shared && yarn recompile-plugins && yarn recompile-plugins-css && react-scripts build",
    "test": "react-scripts test --watch",
    "eject": "react-scripts eject",
    "copy-shared-assets": "mkdir -p ./src/assets/shared/fonts/ && cp -rf ../shared/dist/fonts/* ./src/assets/shared/fonts/.",
    "watch-shared": "node-sass --watch --recursive --output-style expanded  --source-map true ../shared/spark/main.scss ./src/assets/shared/styles/main.css",
    "build-shared": "yarn copy-shared-assets && node-sass --output-style expanded  --source-map true ../shared/src/scss/main.scss ./src/assets/shared/styles/main.css",
    "prettier-src": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "cy:open": "cypress open",
    "cy:run:browser": "cypress run --browser chrome --spec 'cypress/e2e/settings/*.js'",
    "cy:run": "node scripts/cypress-run.js .env.development.local",
    "cy:run:production": "node scripts/cypress-run.js .env.production.local",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "report:coverage": "nyc report --reporter=html",
    "report:coverage:text": "nyc report --reporter=text",
    "report:coverage:summary": "nyc report --reporter=text-summary",
    "open:report": "open-cli ./coverage/lcov-report/index.html",
    "generate:coverage": "nyc report --reporter=lcov",
    "print-env": "print-env",
    "recompile-plugins": "./recompilePlugins.sh",
    "watch-recompile-plugins": "yarn recompile-plugins && watch 'npm run recompile-plugins' ../shared/src/ts",
    "recompile-plugins-css": "node-sass ../shared/src/scss/plugins/plugins-main.scss ./src/assets/shared/styles/plugins.css",
    "watch-plugins-css": "yarn recompile-plugins-css && watch 'npm run recompile-plugins-css' ../shared/src/scss/plugins/plugins"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
